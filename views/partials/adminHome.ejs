            <div class="row">
              <div class="col-md-9 mt-5">
                <div class="card" style="height: 515px; ">
                  <div class="card-body" style="max-height: 700px;">
                    <h5 class="card-title">Chart </h5>
                    <canvas style="max-width: 2200px; max-height: 400px; margin-bottom: 30px; " id="chart" width="668" height="334" ></canvas>
                  </div>
                </div>
              </div>
    
              <div class="col-md-3 mt-5">
                <div class="card" style="height: 515px;">
                  <div class="card-body mt-3">
                      <div class="d-flex justify-content-center py-4">
                        
                        <button type="button" id="startButton" class="btn btn-outline-success me-2">Start</button>
                        <button type="submit" id="stopButton" class="btn btn-outline-danger" >Stop</button>
                      </div>                    
                      

                      <div class="d-flex justify-content-center py-1 mb-2">
                        <select class="form-select me-2" id="timeSeriesSelect">
                          <!-- Options will be added dynamically -->
                        </select>
                        <button type="button" class="btn btn-outline-primary" id="addSeriesButton">Add</button>
                      </div>


                      <div class="d-flex justify-content-center py-1 mb-4">
                        <label for="seriesNumber" class="info-title">Total series: <span id="seriesCount" style="border:0;" class="info-title">0</span></label>
                      </div>
              
                      <div class="d-flex justify-content-center py-1 mb-2" >
                        <select class="form-select me-2" id="datasetSelect">
                          <option selected>Select Series</option>
                          <!-- Options will be added dynamically -->
                        </select>
                        <button type="button" class="btn btn-outline-danger me-2" id="deleteSeriesButton">Delete</button>
                      </div>
                      
                      <input type="text" id="isAdmin" value=" <%= user.IsAdmin%>" hidden>
                      <%if(user.IsAdmin){ %>
                        <!-- this for running the code without any interference -->
                        <input type="text" hidden value="123" id="user-range">
                      <!-- Threshold slider -->
                      <div class="py-2 mb-3">
                        <p>
                          <label for="range" class="info-title">Threshold:</label>
                          <input type="text" id="range" style="border:0;" class="info-title">
                        </p>
                         
                        <div id="slider-range"></div>
                      </div>

                      <!-- Slice Size Selection -->
                      <div class="d-flex justify-content-center py-1 mb-2">
                        <select class="form-select" id="sliceSizeSelect">
                          <option selected >Window Size</option>
                          <option value="5">5</option>
                          <option value="10">10</option>
                          <option value="15">15</option>
                          <option value="20">20</option>
                          <option value="25">25</option>
                          <option value="30">30</option>
                          <option value="35">35</option>
                          <option value="40">40</option>
                          <option value="45">45</option>
                          <option value="50">50</option>
                          <option value="55">55</option>
                          <option value="60">60</option>
                          <option value="65">65</option>
                          <option value="70">70</option>
                          <option value="75">75</option>
                          <option value="80">80</option>
                          <option value="85">85</option>
                          <option value="90">90</option>
                          <option value="95">95</option>
                          <option value="100">100</option>
                        </select>
                      </div>
                      <%}else{ %>
  <!-- Slice Size Selection -->
                      <div class="d-flex justify-content-center py-1 mb-2">
                        <select class="form-select" id="sliceSizeSelect" disabled>
                          <option value="<%=info.WindowSize%>" selected ><%=info.WindowSize%></option>
                        </select>
                      </div>
                      <%}%>
                       <!-- this for running the code without any interference -->
                      <input type="text" hidden value="123" id="range">
                      <input type="text" id="minValueInput" value=" <%- info.min%>" hidden>
                      <input type="text" id="maxValueInput" value=" <%- info.max%>" hidden>
                           <!-- Threshold slider -->
                           <div class="py-2 mb-3">
                            <p>
                              <label for="user-range" class="info-title">Threshold:</label>
                              <input type="text" id="user-range" style="border:0;" class="info-title">
                            </p>
                             
                            <div id="user-slider-range"></div>
                          </div>

                      <div class="d-flex justify-content-center py-1 mb-2">
                        <select class="form-select" id="functionSelect">
                          <option selected disabled>select the function</option>
                          <option value="pearson">regular pearson</option>
                          <option value="pearsonEnhanced">Enhanced Pearson</option>
                        </select>
                      </div>
              
                  </div>
                </div>
              </div>
              
            </div>